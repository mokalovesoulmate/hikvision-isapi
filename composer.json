{
  "name": "mokalovesoulmate/hikvision-isapi",
  "description": "Laravel package for Hikvision ISAPI Face Recognition Terminals integration. Forked from `shaykhnazar/hikvision-isapi`.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "hikvision",
    "isapi",
    "face-recognition",
    "access-control"
  ],
  "authors": [
    {
      "name": "Shaykhnazar",
      "email": "shaykhnazar@gmail.com"
    },
    {
      "name": "Moses Kurniawan",
      "email": "mokalovesoulmate@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.8",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.5|^11.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Shaykhnazar\\HikvisionIsapi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shaykhnazar\\HikvisionIsapi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shaykhnazar\\HikvisionIsapi\\HikvisionIsapiServiceProvider"
      ],
      "aliases": {
        "HikvisionIsapi": "Shaykhnazar\\HikvisionIsapi\\Facades\\HikvisionIsapi"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}